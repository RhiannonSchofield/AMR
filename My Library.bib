@article{tokacInterfacialContributionThickness2015,
  title = {Interfacial Contribution to Thickness Dependent In-Plane Anisotropic Magnetoresistance},
  volume = {5},
  doi = {10.1063/1.4937556},
  abstract = {We have studied in-plane anisotropic magnetoresistance (AMR) in cobalt films with overlayers having designed electrically interface transparency. With an electrically opaque cobalt/overlayer interface, the AMR ratio is shown to vary in inverse proportion to the cobalt film thickness; an indication that in-plane AMR is a consequence of anisotropic scattering with both volume and interfacial contributions. The interface scattering anisotropy opposes the volume scattering contribution, causing the AMR ratio to diminish as the cobalt film thickness is reduced. An intrinsic interface effect explains the significantly reduced AMR ratio in ultra-thin films.},
  number = {12},
