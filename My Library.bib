@article{angelakerisStudyMagnetoresistanceMechanisms2002,
  title = {Study of the {{Magnetoresistance Mechanisms}} in {{Pd}}\textemdash{{Ni Multilayer System}}},
  volume = {189},
  issn = {1521-396X},
  doi = {10.1002/1521-396X(200202)189:2<433::AID-PSSA433>3.0.CO;2-S},
  abstract = {The multilayer modulation of Ni with Pd gives rise to interesting features such as inverted hysteresis, enhanced magnetization and perpendicular magnetic anisotropy development, all strongly influenced by the modulation parameters m, n. In an effort to study the corresponding electrical behavior and its correlation to magnetic features, Pd\textemdash{}Ni multilayers with various m, n, were developed. Since the system does not exhibit the giant magnetoresistance effect, it becomes appropriate for the study of anisotropic magnetoresistance. A series of measurements of electrical resistance and magnetoresistance (MR) at temperatures 10\textendash{}350 K, in samples with 1 {$<$} m, n {$<$} 20 were performed. Four electrical contacts were made on the surface of the samples while the direction of the electric current is parallel to the sample surface (CIP geometry). The dependence of the magnetoresistance on the magnetic field orientation is studied via various experimental set-ups. A simple model to isolate the magnetic term from the over-all electrical resistance is proposed in order to comprehend the magnetic origin of the anisotropic magnetoresistance. Consequently, the temperature dependence and the parametrisation of anisotropic magnetoresistance is examined in Pd\textemdash{}Ni system while the various magnetoresistance contributions arising from different scattering mechanisms may be separated.},
  language = {en},
  number = {2},
  journal = {physica status solidi (a)},
  author = {Angelakeris, M. and Flevaris, N. K.},
  year = {2002},
  keywords = {72.15.Gd,75.50.Cc,75.70.Cn,S1.2},
  pages = {433-437},
  file = {C:\\Users\\rhian\\Zotero\\storage\\WZMQPCXN\\Angelakeris and Flevaris - 2002 - Study of the Magnetoresistance Mechanisms in Pd—Ni.pdf;C:\\Users\\rhian\\Zotero\\storage\\EV7FZD57\\1521-396X(200202)1892433AID-PSSA4333.0.html}
}

@article{hanStructuralMagneticProperties1996,
  title = {Structural, Magnetic Properties and Steep Magnetoresistance Change with Low Saturation Magnetic Fields in {{Co}}/{{Ni}} Multilayer Thin Films},
  volume = {32},
  issn = {0018-9464},
  doi = {10.1109/20.539087},
  abstract = {We have fabricated Co/Ni multilayer thin films with a constant Co thickness of 2.2 nm, Ni thickness changing from 0.5 to 5.0 nm, and a total of 20 bilayers using dc magnetron sputtering. The magnetic measurements show that these multilayers have an in-plane uniaxial anisotropic properties. These multilayers show a steep magnetoresistance change with low saturation magnetic field, an especially large magnetoresistance change between magnetic field H applied parallel and perpendicular to the in-plane easy axis. For H parallel to the multilayer easy axis and H at 45/spl deg/ to the current I, a magnetoresistance ratio of 15.0\% with a FWHM of 13 Oe was obtained. While for H perpendicular to the multilayer easy axis and H at 45/spl deg/ to the current I, a magnetoresistance ratio of 19.1\% with a FWHM of 34 Oe was obtained. The former implies a magnetoresistance sensitivity of 1.15\%/Oe, while the sensitivity of the latter is just 0.56 \%/Oe. All the peaks of the magnetoresistance curve appeared at the coercive field H/sub c/ of the multilayers. The most important characteristic of the magnetoresistance change were a sharp increase in resistance followed by a very steep decrease with increasing H.},
  number = {5},
  journal = {IEEE Transactions on Magnetics},
  author = {Han, D. H.},
  month = sep,
  year = {1996},
  keywords = {0.5 to 5.0 nm,2.2 nm,Anisotropic magnetoresistance,Co-Ni,Co/Ni multilayer thin films,cobalt,dc magnetron sputtering,Enhanced magnetoresistance,ferromagnetic materials,giant magnetoresistance,in-plane uniaxial anisotropic properties,low saturation magnetic fields,magnetic anisotropy,Magnetic anisotropy,Magnetic fields,Magnetic films,magnetic multilayers,Magnetic multilayers,magnetic properties,Magnetic properties,magnetisation,nickel,Perpendicular magnetic anisotropy,Saturation magnetization,sputtered coatings,Sputtering,steep magnetoresistance change,structural properties},
  pages = {4585-4587},
  file = {C:\\Users\\rhian\\Zotero\\storage\\MEDKW3YJ\\Han - 1996 - Structural, magnetic properties and steep magnetor.pdf;C:\\Users\\rhian\\Zotero\\storage\\DDDD2NUH\\539087.html}
